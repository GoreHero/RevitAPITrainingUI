<Window x:Class="RevitAPI_W1_MadeButton.MainView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RevitAPI_W1_MadeButton"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="300">
    <StackPanel>
        <Button Content="Количество труб" Command="{Binding SelectedCommandPipe}"
                Margin="8"/>
        <Button Content="Объем стен" Command="{Binding SelectedCommandWall}"
                Margin="8"/>
        <Button Content="Количество дверей" Command="{Binding SelectedCommandDoor}"
                Margin="8"/>
    </StackPanel>
</Window>
